				int m11 = Integer.parseInt(txt11.getText());
			
				int m13 = Integer.parseInt(txt13.getText());
				
				int m14 = Integer.parseInt(txt14.getText());
				
				float tot = m1 + m2 + m3 + m4 + m5+ m6+ m7 + m8 + m9 + m10 + m11 + m13 + m14 + m16;
				
				double avg = tot/16;
				
				txt12.setText (String.valueOf(avg));
				
				txt17.setText (String.valueOf(tot));
				
		}
	});
	btnNewButton.setBounds(220, 514, 156, 21);
	contentPane.add(btnNewButton);
	}
